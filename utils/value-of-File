import chardet 




    
def get_addresses_from_file():
    file = 'CPdescarga.txt'
    with open(file , 'rb', ) as f:
        resultado = chardet.detect(f.read())
        print(f"Codificaci√≥n detectada: {resultado['encoding']}")
           


print("EHllo reader")
    
get_addresses_from_file()
print("By reader")
    
